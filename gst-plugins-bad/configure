  { echo "$as_me:$LINENO: result: $ac_ct_DUMPBIN" >&5
echo "${ECHO_T}$ac_ct_DUMPBIN" >&6; }
else
  { echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6; }
